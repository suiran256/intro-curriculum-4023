    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "helmet": "^3.8.2",
    "http-errors": "~1.6.2",
    "jquery": "^3.4.1",
    "morgan": "~1.9.0",
    "nodemailer": "^6.4.13",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.9",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "popper.js": "^1.14.0",
    "pug": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "util": "^0.11.1",
    "uuid": "^3.3.2"
