{
  "name": "schedule-arranger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "startOld": "node ./bin/www.mjs",
    "test": "NODE_PATH=${npm_package_config_resolve} jest  --silent --verbose --config ./jest.config.js",
    "coverage": "NODE_PATH=${npm_package_config_resolve} jest --silent --coverage",
    "client": "webpack serve --config ./webpack.config.dev.js",
    "build": "webpack --config ./webpack.config.common.js --mode production",
    "server": "nodemon ./bin/www.mjs  --exec babel-node",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "start": "npm run build && node ./bin/www.mjs"
  },
