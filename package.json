{
  "name": "schedule-arranger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.mjs",
    "test": "NODE_PATH=${npm_package_config_resolve} jest  --silent --verbose --config ./jest.config.js",
    "coverage": "NODE_PATH=${npm_package_config_resolve} jest --silent --coverage",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "typesync",
    "client": "webpack serve --config ./config/webpack.config.dev.js",
    "build": "webpack --config ./config/webpack.config.js --mode production",
    "server": "nodemon ./bin/www.mjs  --exec babel-node",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "startTottoku": "NODE_ENV=production npm run build && npm run server"
  },
  "jest": {},
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.0.0",
