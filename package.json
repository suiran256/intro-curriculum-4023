    "babel-loader": "^8.2.2",
    "babel-node": "^0.0.1-security",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
